{
  "mcpServers": {
    "supabase": {
      "command": "pnpm",
      "args": [
        "exec",
        "mcp-server-supabase",
        "--project-ref",
        "qqewusetilxxfvfkmsed",
        "--service-role-key-auth"
      ],
      "env": {
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}"
      }
    },
    "supabase-migration": {
      "command": "node",
      "args": [
        "mcp-supabase-migration/index.js"
      ],
      "env": {
        "SUPABASE_PROJECT_REF": "qqewusetilxxfvfkmsed",
        "SUPABASE_SERVICE_ROLE_KEY": "${SUPABASE_SERVICE_ROLE_KEY}"
      }
    }
  }
}
