#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/dist/transports/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/dist/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/dist/transports/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/dist/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/mcp-server-supabase/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules/@supabase/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/@supabase+mcp-server-supabase@0.5.6/node_modules:/mnt/c/Users/USER/Downloads/FETS.LIVE-2025/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/transports/stdio.js" "$@"
else
  exec node  "$basedir/../../dist/transports/stdio.js" "$@"
fi
