{
  "name": "fets-point",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm install && (rmdir /s /q node_modules\\.vite-temp || ver > nul) && tsc -b tsconfig.app.json && vite build",
    "build:prod": "pnpm install && (rmdir /s /q node_modules\\.vite-temp || ver > nul) && tsc -b tsconfig.app.json && set BUILD_MODE=prod&& vite build",
    "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -File ../scripts/deploy.ps1 -FtpHost $env:FTP_HOST -User $env:FTP_USER -Password $env:FTP_PASS -RemoteRoot public_html",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "yes | pnpm install && vite preview",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.59.0",
    "@tanstack/react-query-devtools": "^5.59.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.1",
    "lucide-react": "^0.364.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "6.28.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.14.0",
    "jsdom": "^25.0.1",
    "msw": "^2.4.9",
    "postcss": "^8.4.49",
    "react-window": "^1.8.10",
    "supabase": "^2.51.0",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.0.1",
    "vitest": "^3.2.4"
  }
}
